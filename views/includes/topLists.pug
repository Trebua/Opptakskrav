div.topListsContainer
    table.topList
        caption Høyeste poenggrenser for førstegangsvitnemål i #{latestYear}
        thead
            tr
                th(scope="col") #
                th(scope="col") Studienavn
                th(scope="col") Skole
                th(scope="col") Poenggrense
        tbody
            each row in topList.ORDF
                tr
                    td #{row.number}
                    td 
                        a(href=`/${row.code}`) #{row.study}
                    td 
                        a(href=`sok?sokeord=${row.school}`) #{row.school}
                    td #{row.grade.toFixed(1)}
    table.topList
        caption Høyeste poenggrenser for ordinær kvote i #{latestYear}
        thead
            tr
                th(scope="col") #
                th(scope="col") Studienavn
                th(scope="col") Skole
                th(scope="col") Poenggrense
        tbody
            each row in topList.ORD
                tr
                    td #{row.number}
                    td
                        a(href=`/${row.code}`) #{row.study}
                    td
                        a(href=`sok?sokeord=${row.school}`) #{row.school}
                    td #{row.grade.toFixed(1)}